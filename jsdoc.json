{
	"tags": {
		"allowUnknownTags" : true,
		"dictionaries": ["jsdoc","closure"]
	},
	"source": {
		"include": ["src/component.js","README.md"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"opts": {
		"template": "node_modules/egjs-jsdoc-template",
		"destination": "./doc/"
	},
	"plugins": [
		"plugins/markdown",
		"node_modules/egjs-jsdoc-template/jsdoc-plugin/ko",
		"node_modules/egjs-jsdoc-template/jsdoc-plugin/group",
		"node_modules/egjs-jsdoc-template/jsdoc-plugin/codepen",
		"node_modules/egjs-jsdoc-template/jsdoc-plugin/support"
	],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles" : true
		},
		"applicationName": "eg.Component",
		"disqus": "egjs",
		"googleAnalytics": "UA-70842526-14",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "egjs::component - API",
			"description": "A base class utility that manages events and options in modules.",
			"keyword": "UI Component, egjs"
		},
		"linenums": true,
		"link": {
			"canonical": "http://naver.github.io/egjs-component/release/latest/doc/"
		}
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true,
		"tags": ["examples", "ko"]
	}
}
